{
  "name": "Checkpoint",
  "short_name": "Checkpoint",
  "version": "0.0.0",
  "engines": {
    "node": "14.18.3"
  },
  "scripts": {
    "dev": "nodemon --ignore src --ignore server/data -O '{\"module\": \"commonjs\"}' server/index.ts",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entryServer.tsx --outDir dist/server",
    "serve": "cross-env NODE_ENV=production nodemon --ignore server/data -O '{\"module\": \"commonjs\"}' server/index.ts",
    "deploy": "yarn build && yarn serve",
    "preview": "vite preview",
    "cts": "tsc --project . --noEmit",
    "cc": "create-component",
    "gql-codegen": "graphql-codegen --require ./gql-generate-init --documents ",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/google-map-react": "^2.1.4",
    "@types/node-fetch": "^2.6.1",
    "@types/styled-components": "^5.1.21",
    "@urql/exchange-graphcache": "^4.3.6",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "constate": "^3.3.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "express": "^4.17.3",
    "express-useragent": "^1.0.15",
    "google-map-react": "^2.1.10",
    "graphql": "^15.7.2",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.28.0",
    "react-router5": "^8.0.1",
    "react-spring-bottom-sheet": "^3.4.1",
    "react-ssr-prepass": "^1.5.0",
    "react-use": "^17.3.2",
    "recoil": "^0.6.1",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "styled-components": "^5.3.3",
    "swiper": "^6.5.0",
    "swr": "^1.2.2",
    "uuid": "^8.3.2",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/near-operation-file-preset": "^1.13.3",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-operations": "1.13.3",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.0.7",
    "blueprint-templates-cli": "^0.1.13",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "workbox-cli": "^6.5.3"
  }
}
